{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": false
    },
    "hideTitle": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": false
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "sections": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::section.section"
    },
    "seoTitle": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "seoDescription": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "seoImage": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "noIndex": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
